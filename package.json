{
  "name": "react-template",
  "version": "0.0.1",
  "description": "A React/Typescript/Node/PostgreSQL/Docker project template",
  "author": "author",
  "license": "ISC",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "client": "cd packages/client && npm run start",
    "server": "cd packages/server && npm run start",
    "lint": "eslint 'packages/**/*.ts' 'packages/**/*.tsx'",
    "format": "prettier --write 'packages/**/*.ts' 'packages/**/*.tsx'"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3"
  }
}
